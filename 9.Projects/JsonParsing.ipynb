   "source": [
    "# Json Parsing \n",
    "* 결론 : eval()을 하면 굳이 파싱할 필요 없음."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "aaa\n"
     ]
    }
   ],
   "source": [
    "x=\"\"\"[ \n",
    "      {\"id\":1 , \"name\":\"aaa\"}, \n",
    "      {\"id\":2 , \"name\":\"bbb\"} \n",
    " ] \n",
    "\"\"\" \n",
    "xx=eval(x)\n",
    "print(xx[0]['name'])\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1,aaa\n",
      "2,bbb\n"
     ]
    }
   ],
   "source": [
    "for i in xx:\n",
    "    print(\"%d,%s\"%(i[\"id\"],i[\"name\"]))\n",
    " "
   ]
  }
